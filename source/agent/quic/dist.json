{
  "name": "quic-agent",
  "dest": "quic_agent",
  "common": {
      "package": "package.json",
      "files": [
          "agent.toml",
          "log4js_configuration.json",
          "configLoader.js",
          "../index.js",
          "../networkHelper.js",
          "../nodeManager.js",
          "../workingNode.js",
          "../InternalConnectionFactory.js",
          "log4cxx.properties",
          "../../common/amqp_client.js",
          "../../common/cipher.js",
          "../../common/clusterWorker.js",
          "../../common/loadCollector.js",
          "../../common/logger.js",
          "../../common/makeRPC.js",
          "../../common/rpcChannel.js",
          "../../../scripts/release/initcert.js",
          "../../../scripts/release/initauth.js",
          "../../../scripts/detectOS.sh"
      ],
      "folders": {
          "quic": [
              "index.js",
              "../connections.js",
              "../InternalConnectionFactory.js"
          ],
          "quic/p2p": [
              "p2p/p2pQuicStream.js",
              "p2p/p2pQuicTransport.js"
          ],
          "quic/webtransport": [
              "webtransport/quicTransportServer.js"
          ],
          "cert": [
              "../../../cert/.owt.keystore"
          ]
      }
  },
  "natives": {
      "libdist": "lib",
      "addons": [
          {
              "folder": "../addons/quic",
              "name": "quic"
          },
          {
              "folder": "../addons/internalIO",
              "name": "internalIO"
          },
          {
              "folder": "../addons/logger",
              "name": "logger"
          }
      ]
  },
  "start": "node . -U quic"
}
